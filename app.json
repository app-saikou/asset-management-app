{
  "expo": {
    "name": "Polaris ― あなたのお金の羅針盤",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "polaris",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "description": "Polaris（ポラリス）は、あなたのお金の羅針盤となる資産管理アプリです。複利計算により10年後の資産価値を予測し、将来への投資指針を提供します。",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.appsaikou.polaris.finance",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "このアプリはカメラを使用しません。",
        "NSMicrophoneUsageDescription": "このアプリはマイクを使用しません。",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.appsaikou.polaris.finance",
      "versionCode": 1,
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "16.0"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b7be8d51-6857-46ff-9acb-2e401dae8e6e"
      }
    },
    "owner": "y.konishi"
  }
}
